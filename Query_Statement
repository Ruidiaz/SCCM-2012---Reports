select distinct SMS_G_System_COMPUTER_SYSTEM.Name, SMS_R_System.LastLogonUserName, SMS_G_System_COMPUTER_SYSTEM.UserName, SMS_R_System.DeviceOwner, SMS_G_System_INSTALLED_SOFTWARE.ProductName from  SMS_R_System inner join SMS_G_System_COMPUTER_SYSTEM on SMS_G_System_COMPUTER_SYSTEM.ResourceID = SMS_R_System.ResourceId inner join SMS_G_System_INSTALLED_SOFTWARE on SMS_G_System_INSTALLED_SOFTWARE.ResourceID = SMS_R_System.ResourceId where SMS_G_System_INSTALLED_SOFTWARE.ProductName like "Adobe_%" order by SMS_G_System_COMPUTER_SYSTEM.Name DESC
